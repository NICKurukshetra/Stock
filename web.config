  <connectionStrings>
    <add name="cn" connectionString="Data Source=.;User ID='vahan';Password='vahan';Pooling=False;Initial Catalog='FeeCollection'" providerName="System.Data.SqlClient"/>
    <add name="cnstock" connectionString="Data Source=.;User ID='vahan';Password='vahan';Pooling=False;Initial Catalog='FeeCollection'" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <!--
